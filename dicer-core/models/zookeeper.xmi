<?xml version="1.0" encoding="UTF-8"?>
<ddsm:DDSM xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ddsm="http://ddsm/1.0"
    xsi:schemaLocation="http://ddsm/1.0 ../metamodels/ddsm.ecore">
  <cloudelement
      xsi:type="ddsm:ZookeeperCluster"
      elementId="zookeeper">
    <requiresPeerVm
        elementId="requires_peer_1"/>
  </cloudelement>
  <cloudelement
      xsi:type="ddsm:VM"
      elementId="vm1"
      provider="flexiant"
      genericSize="Medium">
    <providedexecutionplatform
        elementId="vm1_hosting"
        owner="vm1"/>
  </cloudelement>
  <cloudelement
      xsi:type="ddsm:ExecutionBinding"
      elementId="peer1_to_host"
      requiredexecutionplatform="requires_peer_1"
      providedexecutionplatform="vm1_hosting"/>
  <cloudelement
      xsi:type="ddsm:Provider"
      elementId="flexiant"/>
</ddsm:DDSM>
