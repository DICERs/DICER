<?xml version="1.0" encoding="UTF-8"?>
<ddsm:DDSM xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ddsm="http://ddsm/1.0" xsi:schemaLocation="http://ddsm/1.0 ../metamodels/ddsm.ecore">
  <cloudelement xsi:type="ddsm:Nimbus" elementId="nimbus">
    <requiredport elementId="requires-zookeeper"/>
    <requiredexecutionplatform elementId="nimbus_requires_host"/>
  </cloudelement>
  <cloudelement xsi:type="ddsm:ZookeeperQuorum" elementId="zookeeper">
    <providedport elementId="provides_zookeeper" owner="zookeeper"/>
  </cloudelement>
  <cloudelement xsi:type="ddsm:Relationship" elementId="storm_to_zookeeper" providedport="provides_zookeeper" requiredport="requires-zookeeper"/>
  <cloudelement xsi:type="ddsm:ZookeeperServer" elementId="server1" belongsToQuorum="zookeeper">
    <requiredexecutionplatform elementId="server1_requires_host"/>
  </cloudelement>
  <cloudelement xsi:type="ddsm:Supervisor" elementId="supervisor1" hasMaster="nimbus">
    <requiredexecutionplatform elementId="supervisor1_requires_host"/>
  </cloudelement>
  <cloudelement xsi:type="ddsm:VM" elementId="vm1" provider="flexiant">
    <providedexecutionplatform elementId="vm1_hosting" owner="vm1"/>
  </cloudelement>
  <cloudelement xsi:type="ddsm:VM" elementId="vm2" provider="flexiant">
    <providedexecutionplatform elementId="vm2_hsoting" owner="vm2"/>
  </cloudelement>
  <cloudelement xsi:type="ddsm:Provider" elementId="flexiant"/>
  <cloudelement xsi:type="ddsm:ExecutionBinding" elementId="supervisor1_to_host" requiredexecutionplatform="supervisor1_requires_host" providedexecutionplatform="vm1_hosting"/>
  <cloudelement xsi:type="ddsm:ExecutionBinding" elementId="nimbus_to_host" requiredexecutionplatform="nimbus_requires_host" providedexecutionplatform="vm2_hsoting"/>
  <cloudelement xsi:type="ddsm:ExecutionBinding" elementId="server1_to_host" requiredexecutionplatform="server1_requires_host" providedexecutionplatform="vm1_hosting"/>
</ddsm:DDSM>
