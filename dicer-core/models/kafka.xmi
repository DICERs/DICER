<?xml version="1.0" encoding="UTF-8"?>
<ddsm:DDSM xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ddsm="http://ddsm/1.0" xsi:schemaLocation="http://ddsm/1.0 ../metamodels/ddsm.ecore">
  <cloudelement xsi:type="ddsm:KafkaCluster" elementId="myKafkaCluster">
    <requiredport elementId="requiresZookeeper"/>
    <requiresPeerVm elementId="broker1"/>
  </cloudelement>
  <cloudelement xsi:type="ddsm:VM" elementId="vm1" provider="myOpenstack">
    <providedexecutionplatform elementId="vm1Hosting" owner="vm1"/>
  </cloudelement>
  <cloudelement xsi:type="ddsm:Provider" elementId="myOpenstack" type="Openstack"/>
  <cloudelement xsi:type="ddsm:ExecutionBinding" elementId="broker1ToVm1" requiredexecutionplatform="broker1" providedexecutionplatform="vm1Hosting"/>
  <cloudelement xsi:type="ddsm:ZookeeperCluster" elementId="myZookeeperCluster">
    <providedport elementId="providesZookeepr" owner="myZookeeperCluster"/>
