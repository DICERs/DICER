  <cloudelement xsi:type="ddsm:VM" elementId="vm2" provider="myOpenstack">
    <providedexecutionplatform elementId="vm2Hosting" owner="vm2"/>
  </cloudelement>
  <cloudelement xsi:type="ddsm:ExecutionBinding" elementId="server1ToVm2" requiredexecutionplatform="server1" providedexecutionplatform="vm2Hosting"/>
  <cloudelement xsi:type="ddsm:Relationship" elementId="kafkaToZookeeper" providedport="providesZookeepr" requiredport="requiresZookeeper"/>
</ddsm:DDSM>
